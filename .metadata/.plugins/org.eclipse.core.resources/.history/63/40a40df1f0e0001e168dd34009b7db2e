package Practice;

public class Abc 
{
	public static void main(String[] args) 
	{
		// Storing a three different string values using one reference variable 
		
		StringBuffer sb=new StringBuffer();
		sb.append("Qspiders?");
		sb.append("Jspiders?");
		sb.append("Pyspiders");
		System.out.println(sb);
	}
}
